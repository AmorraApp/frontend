{
  "name": "@nfinus/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Static site frontend for nfinus.com",
  "type": "module",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "rimraf dist && parcel build",
    "static": "rimraf dist build && parcel build && react-snapshot --build-dir dist --output-dir prod",
    "clear": "rimraf dist prod",
    "lint": "npm run eslint",
    "eslint": "eslint './**/*.{js,jsx}' --fix",
    "stylelint": "stylelint './**/*.{css,scss}'"
  },
  "license": "Private",
  "engines": {
    "node": ">=14.18.0"
  },
  "alias": {
    "common": "./src/common"
  },
  "dependencies": {
    "@popperjs/core": "~2.10.2",
    "bezier-easing": "~2.1.0",
    "binary-search": "~1.3.6",
    "bootstrap": "~5.1.3",
    "case": "~1.6.3",
    "core-js": "~3.19.0",
    "date-fns": "~2.25.0",
    "faker": "~5.5.3",
    "handybars": "~0.4.0",
    "jsonpath-plus": "~5.0.3",
    "lodash": "~4.17.21",
    "memoize-one": "~5.2.1",
    "mobx": "~6.3.5",
    "mobx-react-lite": "~3.2.1",
    "mobx-utils": "~6.0.4",
    "prop-types": "~15.7.2",
    "qs": "~6.10.1",
    "react": "~17.0.2",
    "react-beautiful-dnd": "~13.1.0",
    "react-dom": "~17.0.2",
    "react-router-dom": "~5.3.0",
    "react-select": "~4.3.1",
    "react-sortable-hoc": "~1.11.0",
    "react-syntax-highlighter": "~15.4.4",
    "react-transition-group": "~4.4.2",
    "react-virtualized": "~9.22.3",
    "react-window": "~1.8.6",
    "socket.io-client": "~3.1.1",
    "uncontrollable": "~7.2.1",
    "uuid": "~8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.15.7",
    "@babel/core": "~7.15.8",
    "@babel/eslint-parser": "~7.15.8",
    "@babel/eslint-plugin": "~7.14.5",
    "@babel/plugin-proposal-class-properties": "~7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "~7.15.6",
    "@babel/plugin-transform-runtime": "~7.15.8",
    "@babel/preset-env": "~7.15.8",
    "@babel/preset-react": "~7.14.5",
    "@babel/runtime-corejs3": "~7.15.4",
    "@parcel/config-default": "~2.0.0",
    "@parcel/transformer-sass": "~2.0.0",
    "@parcel/transformer-svg-react": "~2.0.0",
    "autoprefixer": "~10.4.0",
    "babel-plugin-import-glob": "~2.0.0",
    "eslint": "~8.1.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~5.1.1",
    "eslint-plugin-react": "~7.26.1",
    "parcel": "~2.0.0",
    "postcss": "~8.3.11",
    "postcss-custom-properties": "~12.0.0",
    "postcss-import": "~14.0.2",
    "postcss-rem-to-pixel": "~4.1.2",
    "postcss-url": "~10.1.3",
    "react-snapshot": "~1.3.0",
    "rimraf": "~3.0.2",
    "stylelint": "~13.13.1",
    "stylelint-config-recommended": "~5.0.0",
    "stylelint-config-recommended-scss": "~4.3.0",
    "stylelint-scss": "~3.20.1"
  },
  "files": [
    "dist"
  ],
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  }
}
