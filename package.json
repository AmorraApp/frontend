{
  "name": "@nfinus/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Static site frontend for nfinus.com",
  "type": "module",
  "source": "src/index.html",
  "scripts": {
    "start": "rimraf dist && NODE_OPTIONS=--max_old_space_size=4096 node ./dev-stack.mjs",
    "build": "rimraf dist && rollup -c",
    "static": "rimraf dist prod && rollup -c && react-snapshot --build-dir dist --output-dir prod",
    "clean": "rimraf dist prod",
    "lint": "npm run eslint",
    "eslint": "eslint './**/*.{js,jsx}' --fix",
    "stylelint": "stylelint './**/*.{css,scss}'"
  },
  "license": "Private",
  "engines": {
    "node": ">=14.18.0"
  },
  "alias": {
    "common": "./src/common"
  },
  "dependencies": {
    "@apollo/client": "~3.5.7",
    "@popperjs/core": "~2.11.2",
    "bezier-easing": "~2.1.0",
    "bootstrap": "~5.1.3",
    "case": "~1.6.3",
    "core-js": "~3.20.2",
    "date-fns": "~2.28.0",
    "graphql": "~16.2.0",
    "handybars": "~0.4.0",
    "history": "~5.2.0",
    "lodash": "~4.17.21",
    "memoize-one": "~6.0.0",
    "mobx": "~6.3.12",
    "mobx-react-lite": "~3.2.3",
    "mobx-utils": "~6.0.4",
    "prop-types": "~15.8.1",
    "qs": "~6.10.3",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-router-dom": "~6.2.1",
    "react-syntax-highlighter": "~15.4.5",
    "react-transition-group": "~4.4.2",
    "rfs": "~9.0.6",
    "uncontrollable": "~7.2.1",
    "uuid": "~8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.16.8",
    "@babel/core": "~7.16.7",
    "@babel/eslint-parser": "~7.16.0",
    "@babel/eslint-plugin": "~7.16.5",
    "@babel/plugin-proposal-class-properties": "~7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "~7.16.7",
    "@babel/plugin-transform-runtime": "~7.16.8",
    "@babel/preset-env": "~7.16.8",
    "@babel/preset-react": "~7.16.7",
    "@babel/runtime-corejs3": "~7.16.8",
    "@rollup/plugin-alias": "~3.1.9",
    "@rollup/plugin-commonjs": "~21.0.1",
    "@rollup/plugin-html": "~0.2.4",
    "@rollup/plugin-json": "~4.1.0",
    "@rollup/plugin-node-resolve": "~13.1.3",
    "@rollup/plugin-replace": "~3.0.1",
    "autoprefixer": "~10.4.2",
    "babel-plugin-import-glob": "~2.0.0",
    "dotenv": "~11.0.0",
    "eslint": "~8.6.0",
    "eslint-config-twipped": "~6.0.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~6.0.0",
    "eslint-plugin-react": "~7.28.0",
    "express": "~4.17.2",
    "express-http-proxy": "~1.6.3",
    "express-static": "~1.2.6",
    "faker": "~5.5.3",
    "postcss": "~8.4.5",
    "postcss-custom-properties": "~12.0.4",
    "postcss-import": "~14.0.2",
    "postcss-rem-to-pixel": "~4.1.2",
    "postcss-url": "~10.1.3",
    "rimraf": "~3.0.2",
    "rollup": "~2.59.0",
    "rollup-plugin-analyzer": "~4.0.0",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-react-svg": "~3.0.3",
    "rollup-plugin-sizes": "~1.0.4",
    "rollup-plugin-terser": "~7.0.2",
    "sass": "~1.47.0",
    "stylelint": "~14.2.0",
    "stylelint-config-recommended": "~6.0.0",
    "stylelint-config-recommended-scss": "~5.0.2",
    "stylelint-scss": "~4.1.0"
  },
  "files": [
    "dist"
  ],
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  }
}
